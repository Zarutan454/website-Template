<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#06071F"/>
  <rect x="50" y="50" width="700" height="500" fill="#0a0b2a" rx="10" ry="10"/>
  
  <!-- App Header -->
  <rect x="50" y="50" width="700" height="60" fill="#0d0e35" rx="10 10 0 0" ry="10 10 0 0"/>
  <circle cx="85" cy="80" r="20" fill="#00a2ff" opacity="0.3"/>
  <text x="85" y="85" font-family="Arial" font-size="14" fill="#ffffff" text-anchor="middle">👤</text>
  <text x="400" y="85" font-family="Arial" font-size="18" fill="#00a2ff" text-anchor="middle">BSN Mining Rewards</text>
  <circle cx="715" cy="80" r="20" fill="#00a2ff" opacity="0.2"/>
  <text x="715" y="85" font-family="Arial" font-size="14" fill="#ffffff" text-anchor="middle">⚙️</text>
  
  <!-- Mining Status Card -->
  <rect x="80" y="130" width="640" height="150" fill="#0d0e35" rx="10" ry="10"/>
  <rect x="80" y="130" width="640" height="150" fill="url(#gradientMining)" rx="10" ry="10"/>
  
  <!-- Mining Stats -->
  <text x="120" y="165" font-family="Arial" font-size="14" fill="#ffffff" opacity="0.7">Today's Mining Rewards</text>
  <text x="120" y="205" font-family="Arial" font-size="36" fill="#ffffff" font-weight="bold">5.2 BSN</text>
  <text x="120" y="235" font-family="Arial" font-size="16" fill="#ffffff" opacity="0.7">Daily Limit: 10 BSN</text>
  
  <!-- Mining Progress -->
  <rect x="350" y="170" width="350" height="20" rx="10" ry="10" fill="#0d0e35"/>
  <rect x="350" y="170" width="175" height="20" rx="10" ry="10" fill="#00a2ff" opacity="0.6"/>
  <text x="350" y="210" font-family="Arial" font-size="14" fill="#ffffff" opacity="0.7">52% of Daily Limit</text>
  
  <!-- Active Mining Status -->
  <circle cx="680" cy="210" r="10" fill="#00a2ff"/>
  <text x="660" y="210" font-family="Arial" font-size="14" fill="#00a2ff" text-anchor="end">Mining Active</text>
  
  <!-- Mining Rate Graph -->
  <rect x="80" y="300" width="640" height="170" fill="#0d0e35" rx="5" ry="5"/>
  <text x="100" y="330" font-family="Arial" font-size="16" fill="#ffffff">Mining Rate (BSN/hour)</text>
  
  <!-- Graph Grid -->
  <line x1="100" y1="400" x2="700" y2="400" stroke="#ffffff" stroke-width="1" opacity="0.1"/>
  <line x1="100" y1="350" x2="700" y2="350" stroke="#ffffff" stroke-width="1" opacity="0.1"/>
  <line x1="100" y1="450" x2="700" y2="450" stroke="#ffffff" stroke-width="1" opacity="0.1"/>
  
  <line x1="100" y1="350" x2="100" y2="450" stroke="#ffffff" stroke-width="1" opacity="0.1"/>
  <line x1="200" y1="350" x2="200" y2="450" stroke="#ffffff" stroke-width="1" opacity="0.1"/>
  <line x1="300" y1="350" x2="300" y2="450" stroke="#ffffff" stroke-width="1" opacity="0.1"/>
  <line x1="400" y1="350" x2="400" y2="450" stroke="#ffffff" stroke-width="1" opacity="0.1"/>
  <line x1="500" y1="350" x2="500" y2="450" stroke="#ffffff" stroke-width="1" opacity="0.1"/>
  <line x1="600" y1="350" x2="600" y2="450" stroke="#ffffff" stroke-width="1" opacity="0.1"/>
  <line x1="700" y1="350" x2="700" y2="450" stroke="#ffffff" stroke-width="1" opacity="0.1"/>
  
  <!-- Time Labels -->
  <text x="100" y="470" font-family="Arial" font-size="10" fill="#ffffff" opacity="0.5" text-anchor="middle">10:00</text>
  <text x="200" y="470" font-family="Arial" font-size="10" fill="#ffffff" opacity="0.5" text-anchor="middle">11:00</text>
  <text x="300" y="470" font-family="Arial" font-size="10" fill="#ffffff" opacity="0.5" text-anchor="middle">12:00</text>
  <text x="400" y="470" font-family="Arial" font-size="10" fill="#ffffff" opacity="0.5" text-anchor="middle">13:00</text>
  <text x="500" y="470" font-family="Arial" font-size="10" fill="#ffffff" opacity="0.5" text-anchor="middle">14:00</text>
  <text x="600" y="470" font-family="Arial" font-size="10" fill="#ffffff" opacity="0.5" text-anchor="middle">15:00</text>
  <text x="700" y="470" font-family="Arial" font-size="10" fill="#ffffff" opacity="0.5" text-anchor="middle">16:00</text>
  
  <!-- Rate Labels -->
  <text x="80" y="450" font-family="Arial" font-size="10" fill="#ffffff" opacity="0.5" text-anchor="end">0.5</text>
  <text x="80" y="400" font-family="Arial" font-size="10" fill="#ffffff" opacity="0.5" text-anchor="end">1.0</text>
  <text x="80" y="350" font-family="Arial" font-size="10" fill="#ffffff" opacity="0.5" text-anchor="end">1.5</text>
  
  <!-- Graph Data -->
  <path d="M100,420 C150,410 200,380 250,390 C300,400 350,370 400,350 C450,330 500,360 550,370 C600,380 650,360 700,340" 
        stroke="#00a2ff" stroke-width="3" fill="none"/>
  
  <!-- Data Points -->
  <circle cx="100" cy="420" r="4" fill="#00a2ff"/>
  <circle cx="200" cy="380" r="4" fill="#00a2ff"/>
  <circle cx="300" cy="390" r="4" fill="#00a2ff"/>
  <circle cx="400" cy="350" r="4" fill="#00a2ff"/>
  <circle cx="500" cy="360" r="4" fill="#00a2ff"/>
  <circle cx="600" cy="380" r="4" fill="#00a2ff"/>
  <circle cx="700" cy="340" r="4" fill="#00a2ff"/>
  
  <!-- Boost Events -->
  <circle cx="300" cy="390" r="8" fill="none" stroke="#00a2ff" stroke-width="2"/>
  <text x="300" y="375" font-family="Arial" font-size="10" fill="#00a2ff" text-anchor="middle">Post</text>
  
  <circle cx="500" cy="360" r="8" fill="none" stroke="#00a2ff" stroke-width="2"/>
  <text x="500" y="345" font-family="Arial" font-size="10" fill="#00a2ff" text-anchor="middle">Comment</text>
  
  <circle cx="700" cy="340" r="8" fill="none" stroke="#00a2ff" stroke-width="2"/>
  <text x="700" y="325" font-family="Arial" font-size="10" fill="#00a2ff" text-anchor="middle">Invite</text>
  
  <!-- Boost Cards -->
  <rect x="80" y="490" width="200" height="60" fill="#0d0e35" rx="5" ry="5"/>
  <circle cx="110" cy="520" r="15" fill="#00a2ff" opacity="0.2"/>
  <text x="110" y="525" font-family="Arial" font-size="14" fill="#ffffff" text-anchor="middle">📝</text>
  <text x="135" y="510" font-family="Arial" font-size="14" fill="#ffffff">Post Boost</text>
  <text x="135" y="530" font-family="Arial" font-size="14" fill="#00a2ff">+0.5 BSN</text>
  
  <rect x="300" y="490" width="200" height="60" fill="#0d0e35" rx="5" ry="5"/>
  <circle cx="330" cy="520" r="15" fill="#00a2ff" opacity="0.2"/>
  <text x="330" y="525" font-family="Arial" font-size="14" fill="#ffffff" text-anchor="middle">💬</text>
  <text x="355" y="510" font-family="Arial" font-size="14" fill="#ffffff">Comment Boost</text>
  <text x="355" y="530" font-family="Arial" font-size="14" fill="#00a2ff">+0.2 BSN</text>
  
  <rect x="520" y="490" width="200" height="60" fill="#0d0e35" rx="5" ry="5"/>
  <circle cx="550" cy="520" r="15" fill="#00a2ff" opacity="0.2"/>
  <text x="550" y="525" font-family="Arial" font-size="14" fill="#ffffff" text-anchor="middle">👥</text>
  <text x="575" y="510" font-family="Arial" font-size="14" fill="#ffffff">Invite Boost</text>
  <text x="575" y="530" font-family="Arial" font-size="14" fill="#00a2ff">+1.0 BSN</text>
  
  <!-- Gradient Definition -->
  <defs>
    <linearGradient id="gradientMining" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0d0e35;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00a2ff;stop-opacity:0.1" />
    </linearGradient>
  </defs>
</svg> 