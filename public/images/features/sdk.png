  <!-- Code -->
  <text x="120" y="180" font-family="monospace" font-size="12" fill="#ff79c6">import</text>
  <text x="165" y="180" font-family="monospace" font-size="12" fill="#f8f8f2"> { BSNClient } </text>
  <text x="270" y="180" font-family="monospace" font-size="12" fill="#ff79c6">from</text>
  <text x="300" y="180" font-family="monospace" font-size="12" fill="#f1fa8c">'@bsn/sdk'</text>
  <text x="370" y="180" font-family="monospace" font-size="12" fill="#f8f8f2">;</text>
  
  <text x="120" y="220" font-family="monospace" font-size="12" fill="#ff79c6">const</text>
  <text x="155" y="220" font-family="monospace" font-size="12" fill="#f8f8f2"> client = </text>
  <text x="220" y="220" font-family="monospace" font-size="12" fill="#ff79c6">new</text>
  <text x="245" y="220" font-family="monospace" font-size="12" fill="#f8f8f2"> BSNClient({</text>
  <text x="140" y="240" font-family="monospace" font-size="12" fill="#f8f8f2">  apiKey: </text>
  <text x="195" y="240" font-family="monospace" font-size="12" fill="#f1fa8c">'YOUR_API_KEY'</text>
  <text x="300" y="240" font-family="monospace" font-size="12" fill="#f8f8f2">,</text>
  <text x="140" y="260" font-family="monospace" font-size="12" fill="#f8f8f2">  environment: </text>
  <text x="220" y="260" font-family="monospace" font-size="12" fill="#f1fa8c">'production'</text>
  <text x="140" y="280" font-family="monospace" font-size="12" fill="#f8f8f2">});</text>
  
  <text x="120" y="320" font-family="monospace" font-size="12" fill="#ff79c6">async function</text>
  <text x="210" y="320" font-family="monospace" font-size="12" fill="#50fa7b"> createPost</text>
  <text x="270" y="320" font-family="monospace" font-size="12" fill="#f8f8f2">() {</text>
  <text x="140" y="340" font-family="monospace" font-size="12" fill="#ff79c6">  try</text>
  <text x="160" y="340" font-family="monospace" font-size="12" fill="#f8f8f2"> {</text>
  <text x="140" y="360" font-family="monospace" font-size="12" fill="#f8f8f2">    const post = </text>
  <text x="225" y="360" font-family="monospace" font-size="12" fill="#ff79c6">await</text>
  <text x="255" y="360" font-family="monospace" font-size="12" fill="#f8f8f2"> client.posts.create({</text>
  <text x="160" y="380" font-family="monospace" font-size="12" fill="#f8f8f2">      content: </text>
  <text x="225" y="380" font-family="monospace" font-size="12" fill="#f1fa8c">'Hello BSN world!'</text>
  <text x="350" y="380" font-family="monospace" font-size="12" fill="#f8f8f2">,</text>
  <text x="160" y="400" font-family="monospace" font-size="12" fill="#f8f8f2">      tags: [</text>
  <text x="210" y="400" font-family="monospace" font-size="12" fill="#f1fa8c">'BSN'</text>
  <text x="240" y="400" font-family="monospace" font-size="12" fill="#f8f8f2">, </text>
  <text x="250" y="400" font-family="monospace" font-size="12" fill="#f1fa8c">'SDK'</text>
  <text x="280" y="400" font-family="monospace" font-size="12" fill="#f8f8f2">]</text>
  <text x="160" y="420" font-family="monospace" font-size="12" fill="#f8f8f2">    });</text>
  <text x="140" y="440" font-family="monospace" font-size="12" fill="#f8f8f2">    </text>
  <text x="160" y="440" font-family="monospace" font-size="12" fill="#ff79c6">return</text>
  <text x="195" y="440" font-family="monospace" font-size="12" fill="#f8f8f2"> post;</text>
  <text x="140" y="460" font-family="monospace" font-size="12" fill="#f8f8f2">  } </text>
  <text x="160" y="460" font-family="monospace" font-size="12" fill="#ff79c6">catch</text>
  <text x="190" y="460" font-family="monospace" font-size="12" fill="#f8f8f2">(error) {</text>
  <text x="160" y="480" font-family="monospace" font-size="12" fill="#f8f8f2">    console.error(error);</text>
  <text x="140" y="500" font-family="monospace" font-size="12" fill="#f8f8f2">  }</text>
  <text x="120" y="520" font-family="monospace" font-size="12" fill="#f8f8f2">}</text>
  
